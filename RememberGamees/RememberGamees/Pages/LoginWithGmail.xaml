<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RememberGamees.Pages.LoginWithGmail">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="ImgPic" MinimumHeightRequest="200" MinimumWidthRequest="200"/>
            <Label x:Name="Name"/>
            <Label x:Name="Email"/>
            <Button x:Name="GoToGamePge" Text="Go to play" Clicked="GamePageNav_Clicked"></Button>
            <WebView x:Name="WebView" HeightRequest="10" WidthRequest="10" VerticalOptions="FillAndExpand"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>