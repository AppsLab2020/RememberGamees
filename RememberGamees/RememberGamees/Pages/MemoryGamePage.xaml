<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RememberGamees.Pages.MemoryGamePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Experiences2_Text}" />
            <Label Text="{Binding OldRandImageInLabel}"/>
            <Label Text="{Binding NewRandImageInLabel}"/>
            <Button Text="Yes"
                    Command="{Binding Yes_Clicked}"/>
            <Button Text="No"
                    Command="{Binding No_Clicked}"/>
            <Image Source="{Binding RandomImage_Source}"   WidthRequest="100" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>