<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RememberGamees.Pages.MemoryGamePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Is this shape matching with previous?"/>
            <Button Text="Yes"
                    Command="{Binding Yes_Clicked}"/>
            <Button Text="No"
                    Command="{Binding No_Clicked}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>